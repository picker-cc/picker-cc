{
  "name": "@picker-cc/admin-ui-plugin",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:csr": "ts-node scripts/build-csr",
    "build:ssr": "ts-node scripts/build-ssr",
    "postbuild": "npm run build:csr && npm run build:ssr",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "watch": "nest start --watch",
    "debug": "nest start --debug --watch",
    "prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "homepage": "https://www.picker.cc/",
  "funding": "https://github.com/sponsors/baisheng",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@iconify-json/bx": "^1.1.3",
    "@picker-cc/common": "workspace:^1.0.0",
    "@picker-cc/core": "workspace:^1.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.8",
    "@types/fs-extra": "^9.0.1",
    "@types/shelljs": "^0.8.11",
    "express": "^4.17.1",
    "rimraf": "^3.0.2",
    "ssr": "^6.2.34",
    "ssr-plugin-nestjs": "^6.2.20",
    "ssr-plugin-vue3": "^6.2.66",
    "ssr-types": "^6.2.26",
    "typescript": "4.8.3",
    "unocss": "^0.45.21"
  },
  "dependencies": {
    "@nestjs/common": "^9.1.2",
    "@nestjs/core": "^9.1.2",
    "@nestjs/mapped-types": "^1.1.0",
    "@nestjs/platform-express": "^9.1.2",
    "@picker-ui/core": "workspace:^1.0.0",
    "@unocss/reset": "^0.45.21",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/server-renderer": "^3.2.39",
    "@vueuse/core": "^9.2.0",
    "anu-vue": "^0.8.0",
    "compression": "^1.7.4",
    "fs-extra": "^10.0.0",
    "pinia": "^2.0.22",
    "reflect-metadata": "^0.1.13",
    "rollup": "^2.79.0",
    "rxjs": "^7.5.5",
    "source-map-support": "^0.5.21",
    "ssr-common-utils": "^6.2.46",
    "ssr-core-vue3": "^6.2.18",
    "vite": "^3.1.3",
    "vite-plugin-ssr": "^0.4.38",
    "vue": "3.2.39",
    "vue-router": "^4.1.5"
  }
}
